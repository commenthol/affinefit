{
  "name": "affinefit",
  "version": "1.0.2",
  "description": "Fit an affine transformation to given points",
  "main": "lib/affineFit.js",
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": "commenthol <commenthol@gmail.com>",
  "license": "CC0-1.0",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0"
  },
  "scripts": {
    "test": "mocha test/*.mocha.js",
    "cover": "istanbul cover _mocha --report lcov --report text -- --reporter dot --check-leaks test/*.mocha.js",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "lint": "eslint '**/*.js'",
    "pack": "npm test && npm run lint && npm run readme && npm pack",
    "babel": "babel --presets=es2015 -d lib/ src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/affinefit.git"
  },
  "keywords": [
    "affine",
    "transformation"
  ],
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ],
    "rules": {
      "no-multi-spaces": 0
    }
  }
}

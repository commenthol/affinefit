{
  "name": "affinefit",
  "version": "1.0.5",
  "description": "Fit an affine transformation to given points",
  "keywords": [
    "affine",
    "transformation"
  ],
  "homepage": "https://github.com/commenthol/affinefit",
  "bugs": {
    "url": "https://github.com/commenthol/affinefit/issues"
  },
  "license": "CC0-1.0",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "lib/affineFit.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/affinefit.git"
  },
  "scripts": {
    "all": "npm run clean && npm run lint && npm run build && npm test",
    "build": "babel -d lib src",
    "clean": "rimraf lib es coverage .nyc_output",
    "clean:all": "rimraf node_modules && npm run clean",
    "coverage": "nyc -r text -r html npm test",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ],
    "rules": {
      "no-multi-spaces": 0
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ]
}
